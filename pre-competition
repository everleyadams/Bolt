//Driver:Evan
//Navigator:Everley


	await scrollMatrixText('Start!', { r: 43, g: 211, b: 255 }, 15, true);
	await speak("Start", true);


//Red
//Bolt LED will turn Red
//Driver:Everley
//Navigator:Evan

 	setMainLed({ r: 255, g: 0, b: 0 });
//Forward & turn 90 degrees and forward again
//Driver:Evan
//Navigator:Everley




//Play Sound
//Driver:Everley
//Navigator:Evan


//End Program
}
